    "playwright": "^1.48.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
