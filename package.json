{
  "name": "playwrong",
  "version": "1.0.0",
  "description": "Browser Control MCP Server for Gemini Code Assistant",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "playwrong": "./dist/stdio.js"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node --esm src/index.ts",
    "build": "tsc",
